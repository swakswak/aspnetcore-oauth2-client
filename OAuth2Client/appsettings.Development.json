{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "OAuth": {
    "Kakao": {
      "ClientId": "{my-kakao-oauth-client-id}",
      "ClientSecret": "{my-kakao-oauth-client-secret}",
      "SaveTokens": false,
      "Scopes": [
        "profile_nickname",
        "profile_image",
        "account_email",
        "birthday"
      ]
    }
  },
  "Jwt": {
    "SecretKey": "{my-jwt-secret-key-example}",
    "Issuer": "MyOAuthSampleService",
    "Audience": "MyOAuthSampleServiceUser",
    "ExpirationMinutes": 30,
    "TokenName": "AccessToken"
  }
}
